      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "// –ü—Ä–æ–≤–µ—Ä—è–µ–º —É—Å–ø–µ—à–Ω—ã–π –æ—Ç–≤–µ—Ç",
              "pm.test(\"–°—Ç–∞—Ç—É—Å –æ—Ç–≤–µ—Ç–∞ 200\", function() {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "// –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–∞–ª–∏—á–∏–µ —Ç–æ–∫–µ–Ω–∞ –≤ –æ—Ç–≤–µ—Ç–µ",
              "pm.test(\"–û—Ç–≤–µ—Ç —Å–æ–¥–µ—Ä–∂–∏—Ç JWT —Ç–æ–∫–µ–Ω\", function() {",
              "    var jsonData = pm.response.json();",
              "    pm.expect(jsonData.token).to.exist;",
              "    pm.expect(jsonData.token).to.be.a('string');",
              "    pm.expect(jsonData.token.length).to.be.greaterThan(0);",
              "});",
              "",
              "// –°–æ—Ö—Ä–∞–Ω—è–µ–º —Ç–æ–∫–µ–Ω –≤ –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –∫–æ–ª–ª–µ–∫—Ü–∏–∏",
              "var jsonData = pm.response.json();",
              "if (jsonData.token) {",
              "    pm.collectionVariables.set(\"jwt_token\", jsonData.token);",
              "    console.log(\"JWT —Ç–æ–∫–µ–Ω —É—Å–ø–µ—à–Ω–æ —Å–æ—Ö—Ä–∞–Ω–µ–Ω –≤ –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –∫–æ–ª–ª–µ–∫—Ü–∏–∏\");",
              "}"
            ],
            "type": "text/javascript"
          }
        }
